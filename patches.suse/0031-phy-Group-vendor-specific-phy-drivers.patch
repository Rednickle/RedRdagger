-	writel(readl(base + PHY_28NM_HSIC_PLL_CTRL2) &
-		~PHY_28NM_HSIC_S2H_PU_PLL,
-		base + PHY_28NM_HSIC_PLL_CTRL2);
-
-	clk_disable_unprepare(mv_phy->clk);
-	return 0;
-}
