-static void rockchip_usb_phy_action(void *data)
-{
-	struct rockchip_usb_phy *rk_phy = data;
-
-	if (!rk_phy->uart_enabled) {
-		of_clk_del_provider(rk_phy->np);
-		clk_unregister(rk_phy->clk480m);
-	}
-
-	if (rk_phy->clk)
-		clk_put(rk_phy->clk);
-}
