-			dev_err(&pdev->dev,
-				"couldn't get power reg. offset\n");
-			return -EINVAL;
-		}
-	}
-
-	otg->set_host		= omap_usb_set_host;
-	otg->set_peripheral	= omap_usb_set_peripheral;
-	if (phy_data->flags & OMAP_USB2_HAS_SET_VBUS)
