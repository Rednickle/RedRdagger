-
-static int rockchip_usb_phy_power_on(struct phy *_phy)
-{
-	struct rockchip_usb_phy *phy = phy_get_drvdata(_phy);
-
-	if (phy->uart_enabled)
-		return -EBUSY;
-
-	if (phy->vbus) {
-		int ret;
-
-		ret = regulator_enable(phy->vbus);
-		if (ret)
-			return ret;
-	}
-
-	return clk_prepare_enable(phy->clk480m);
-}
