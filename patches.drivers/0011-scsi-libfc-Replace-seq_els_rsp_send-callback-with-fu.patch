 	fc_frame_free(rx_fp);
 }
 
@@ -1419,7 +1419,7 @@ static void fc_rport_recv_rtv_req(struct fc_rport_priv *rdata,
 	if (!fp) {
 		rjt_data.reason = ELS_RJT_UNAB;
